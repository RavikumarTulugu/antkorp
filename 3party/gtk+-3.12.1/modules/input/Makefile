install-dist_imconffileDATA: $(dist_imconffile_DATA)
	@$(NORMAL_INSTALL)
	@list='$(dist_imconffile_DATA)'; test -n "$(imconffiledir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(imconffiledir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(imconffiledir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(imconffiledir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(imconffiledir)" || exit $$?; \
	done

uninstall-dist_imconffileDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(dist_imconffile_DATA)'; test -n "$(imconffiledir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(imconffiledir)'; $(am__uninstall_files_from_dir)

